		<!-- HOLON LEVEL -->

		<!--On Page load all the objects from the database will load on Map (Lines, 
			Objects, PowerSource) -->
		<!-- <action name="showHolonObjects" class="com.htc.action.HolonObjectAction" 
			method="showHolonObjects" /> instead of this we can use below -->
		<action name="showHolons" class="com.htc.action.HolonAction" method="showHolons" />

		<!-- HOLON OBJECT LEVEL -->

		<!--To create New Holon Object -->
		<action name="createHolonObject" class="com.htc.action.HolonObjectAction"
			method="createHolonObject" />
			<action name="getHolonObjectInfoWindow" class="com.htc.action.HolonObjectAction"
			method="getHolonObjectInfoWindow" />

		<!--To Edit Existing Holon Object -->
		<!-- <action name="editHolonObject" class="com.htc.action.HolonObjectAction" 
			method="editHolonObject" /> Instead to one method we added to put below four -->
		<action name="showElementsInHolonObject" class="com.htc.action.HolonObjectAction"
			method="showElementsInHolonObject" />
		<action name="addElementToHolonObject" class="com.htc.action.HolonObjectAction"
			method="addElementToHolonObject" />
		<action name="editElementInHolonObject" class="com.htc.action.HolonObjectAction"
			method="editElementInHolonObject" />
		<action name="deleteElementFromHolonObject" class="com.htc.action.HolonObjectAction"
			method="deleteElementFromHolonObject" />
			
			<!-- Show Details of Holon Objects -->
		<action name="showHolonObjectDetails" class="com.htc.action.HolonObjectAction"
			method="showHolonObjectDetails" />
			
			
		<!-- To connect a holon object to main line with the help of subline-->
		<action name="connectToPowerSource" class="com.htc.action.PowerLineAction"
			method="connectToPowerSource" />

		<action name="showHolonObjectConsumption" class="com.htc.action.ConsumptionGraphAction"
			method="showConsumptionGraph" />



		<!--SWITCH LEVEL -->
		<action name="createPowerSwitch" class="com.htc.action.PowerSwitchAction"
			method="createPowerSwitch" />
		<action name="getSwitchInfo" class="com.htc.action.PowerSwitchAction"
			method="getSwitchInfo" />
		<action name="getListPowerSwitch" class="com.htc.action.PowerSwitchAction"
			method="getListPowerSwitch" />
		<action name="powerSwitchOnOff" class="com.htc.action.PowerSwitchAction"
			method="powerSwitchOnOff" />
			
			<!--POWER LINE LEVEL  -->
			<action name="drawPowerLine" class="com.htc.action.PowerLineAction"
			method="drawPowerLine" /> <!-- Completed By Abhinav and Code Checked in. Enhancements could be done later  -->			
			<action name="showPowerLines" class="com.htc.action.PowerLineAction"
			method="showPowerLines" /> <!-- Completed By Abhinav and Code Checked in. Enhancements could be done later  -->
			<action name="getPowerLineInfo" class="com.htc.action.PowerLineAction"
			method="getPowerLineInfo" />
			<action name="updatePowerLine" class="com.htc.action.PowerLineAction"
			method="updatePowerLine" />
			
		<action name="dataGenerator" class="com.htc.action.DataGeneratorAction"
