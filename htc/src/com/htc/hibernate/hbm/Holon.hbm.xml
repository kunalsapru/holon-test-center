<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.htc.hibernate.pojo.Holon" table="holon" optimistic-lock="version">
        <id name="id" type="int">
            <column name="id" />
            <generator class="increment" />
        </id>
        <property name="name" type="string">
            <column name="name" />
        </property>
        <property name="radius" type="java.lang.Integer">
            <column name="radius" />
        </property>
        <property name="holonCenter" type="java.lang.Integer">
            <column name="holon_center" unique="true" />
        </property>
    </class>
</hibernate-mapping>
